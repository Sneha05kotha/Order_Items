<h1 class="mt-5">Register</h1>
<form method="POST" action="/user">
  <div class="form-group row mt-4">
    <label for="inputjobName" class="col-sm-2 col-form-label">Username:</label>
    <div class="col-sm-4">
      <input
        type="text"
        class="form-control"
        id="inputjobName"
        name="username"
        placeholder="Username"
      />
    </div>
  </div>
  <div class="form-group row mt-4">
    <label for="inputjobName" class="col-sm-2 col-form-label"
      >Account Number:</label
    >
    <div class="col-sm-4">
      <input
        type="text"
        class="form-control"
        id="inputjobName"
        name="accountno"
        placeholder="Account no."
      />
    </div>
  </div>
  <div class="form-group row mt-4">
    <label for="inputjobName" class="col-sm-2 col-form-label">Address:</label>
    <div class="col-sm-4">
      <input
        type="text"
        class="form-control"
        id="inputjobName"
        name="address"
        placeholder="Address"
      />
    </div>
  </div>
  <div class="form-group row mt-4">
    <label for="inputjobName" class="col-sm-2 col-form-label"
      >Phone Number:</label
    >
    <div class="col-sm-4">
      <input
        type="text"
        class="form-control"
        id="inputjobName"
        name="phoneno"
        placeholder="Phone no."
      />
    </div>
  </div>
  <div class="form-group row mt-4">
    <label for="inputjobName" class="col-sm-2 col-form-label">Email-ID:</label>
    <div class="col-sm-4">
      <input
        type="email"
        class="form-control"
        id="inputjobName"
        name="email"
        placeholder="Email Address"
      />
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPartID" class="col-sm-2 col-form-label">Password:</label>
    <div class="col-sm-4">
      <input
        type="password"
        class="form-control"
        id="inputjobName"
        name="password"
        placeholder="Password"
      />
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-primary px-4">Submit</button>
    </div>
  </div>
</form>
<a href="/login">Already a member? Login here!!</a>
