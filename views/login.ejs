<h1 class="mt-5">Login</h1>
<form method="POST" action="/login?url=<%= url %>">
  <div class="form-group row mt-4">
    <label for="inputjobName" class="col-sm-2 col-form-label">UserId:</label>
    <div class="col-sm-4">
      <input
        type="text"
        class="form-control"
        id="inputjobName"
        name="username"
        placeholder="UserId"
      />
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPartID" class="col-sm-2 col-form-label">Password:</label>
    <div class="col-sm-4">
      <input
        type="password"
        class="form-control"
        id="inputjobName"
        name="password"
        placeholder="Password"
      />
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-primary px-4">Submit</button>
    </div>
  </div>
</form>
<a href="/register">Not a member? Register here!!</a>
